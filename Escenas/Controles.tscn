script/source = "extends Node2D

func _ready():
	get_node(\"Sprite\").edit_set_rect(Rect2(0,0,OS.get_window_size().width/1.5,OS.get_window_size().height/1.2))
	get_node(\"Sprite\").set_global_pos(Vector2(OS.get_window_size().width,OS.get_window_size().height)/2)
	get_node(\"Salir\").set_global_pos(Vector2(0 + get_node(\"Salir\").get_rect().size.width,OS.get_window_size().height/1.1))
	get_node(\"Siguiente\").set_global_pos(Vector2(OS.get_window_size().width - get_node(\"Siguiente\").get_rect().size.width-get_node(\"Salir\").get_rect().size.width,OS.get_window_size().height/1.1))
	get_node(\"Panel\").edit_set_rect(Rect2(0,0,OS.get_window_size().width,OS.get_window_size().height))
	pass


func _on_Salir_pressed():
	get_tree().change_scene(\"res://Escenas/MainMenu.tscn\")
	pass # replace with function body


func _on_Siguiente_pressed():
	get_tree().change_scene(\"res://Escenas/Seleccion_Personajes.tscn\")
	pass # replace with function body
"

