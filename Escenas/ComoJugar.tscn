	get_node(\"Recco\").set_pos(Vector2(width/2-get_node(\"Recco\").get_rect().size.x,height/1.4))
	pass


func _on_Siguiente_pressed():
	get_tree().change_scene(\"res://Escenas/Controles.tscn\")
	pass # replace with function body
